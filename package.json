{
  "name": "yumecoder_webapp",
  "version": "0.0.1",
  "description": "online IT practice platform",
  "main": "index.js",
  "author": "yuusha",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --port 3000 --hot --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "build:sit": "cross-env AXIOS_ENV=sit webpack --config webpack.prod.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir types",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check --watch",
    "eslint-check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "stylelint-check": "stylelint ./src",
    "lint": "yarn run type-check && yarn run eslint-check && yarn run stylelint-check",
    "test:unit": "cross-env NODE_ENV=unittest jest ./test/unittest",
    "test:sit": "cross-env NODE_ENV=sit jest ./test/sit",
    "test": "yarn run test:unit && yarn run test:sit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run type-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint",
    "src/**/*.{css,scss,sass,less}": "stylelint",
    "*": "prettier --check"
  },
  "browserslist": [
    "defaults",
    "> 0.2%",
    "not op_mini all"
  ],
  "config": {
    "publicPath": "/"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/enzyme": "^3.10.5",
    "@types/fastclick": "^1.0.28",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/raf": "^3.4.0",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/vfile-message": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "obsolete-webpack-plugin": "^0.5.6",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "1.18.2",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "typescript": "^3.8.3",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "axios": "^0.19.2",
    "check-browser-info": "^1.1.3",
    "core-js": "^3.6.4",
    "lodash": "^4.17.15",
    "raf": "^3.4.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-intl": "^3.9.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.5",
    "typeface-roboto": "^0.0.75"
  }
}
